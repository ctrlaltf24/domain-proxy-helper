{
  "manifest_version": 2,
  "name": "Containers Socks Proxy Helper",
  "description": "Assign a separate socks proxy to each Firefox container and to Private browsing windows - be sure your privacy is protected!",
  "version": "0.0.3b",
  "icons": {
    "48": "icons/locked.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "containers-proxy-helper@firefox4ever",
      "strict_min_version": "91.1.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  },
  "options_ui": {
    "browser_style": true,
    "page": "options/options.html"
  },
  "permissions": [
    "proxy",
    "storage",
    "contextualIdentities",
    "<all_urls>"
  ]
}
